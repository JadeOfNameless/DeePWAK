\begin{algorithm}[H]
  \caption{DeePWAK constructor}
  \DontPrintSemicolon  % Don't print semicolons

  \Dat{\DeePWAK}{$\mathcal{E},\mathcal{P},\mathcal{D}$}{

    {$\forall m$ : $\mathbb{N}$ \tcp*{input parameter dimension}}
    {$\forall d$ : $\mathbb{N}$ \tcp*{embedding dimension}}
    {$\forall k$ : $\mathbb{N}$ \tcp*{maximum number of clusters}}
    \KwIn{}
    $\mathcal{E}$ : $\Arch{m,d}$ \tcp*{encoder architecture}
    $\mathcal{P}$ : $\Arch{m,k}$ \tcp*{partitioner architecture}
    $\mathcal{D}$ : $\Arch{d,m}$ \tcp*{decoder architecture}
    
